<p id="notice"><%= notice %></p>

<h1>Listing Api Rooms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_rooms.each do |api_room| %>
      <tr>
        <td><%= link_to 'Show', api_room %></td>
        <td><%= link_to 'Edit', edit_api_room_path(api_room) %></td>
        <td><%= link_to 'Destroy', api_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room', new_api_room_path %>
