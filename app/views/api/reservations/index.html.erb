<p id="notice"><%= notice %></p>

<h1>Listing Api Reservations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_reservations.each do |api_reservation| %>
      <tr>
        <td><%= link_to 'Show', api_reservation %></td>
        <td><%= link_to 'Edit', edit_api_reservation_path(api_reservation) %></td>
        <td><%= link_to 'Destroy', api_reservation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reservation', new_api_reservation_path %>
